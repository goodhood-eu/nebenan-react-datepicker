{
  "name": "nebenan-react-datepicker",
  "description": "React datepicker component",
  "author": "Nebenan.de <peter.h@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-datepicker",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-datepicker/issues",
  "version": "2.0.0",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install && npm run decache",
    "decache": "find ./build -type f -name '.browserify-cache.json' -delete || echo \"\\033[0;32mAlready decached!\\033[0m\"",
    "check": "npm run test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "gulp": "gulp",
    "compile": "NODE_ENV=production npm run gulp compile",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run build",
    "lint": "stylelint \"./*.scss\" \"./**/*.scss\" && eslint --ext .jsx,.js ./",
    "lint:tofile": "eslint --ext .jsx,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' NODE_ENV=test mocha \"test/**/*.js\"",
    "test:tofile": "NODE_ENV=production npm run gulp compile && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "start": "npm run gulp"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.2.1",
    "babel-plugin-transform-node-env-inline": "0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "browserify-incremental": "^3.1.1",
    "browserslist-config-nebenan": "^1.0.0",
    "chai": "^4.2.0",
    "clsx": "^1.1.1",
    "core-js": "^3.8.2",
    "date-fns": "^2.16.1",
    "del": "^6.0.0",
    "enzyme": "^3.11.0",
    "errorhandler": "^1.5.1",
    "eslint": "^7.17.0",
    "eslint-config-nebenan": "^3.1.0",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "nebenan-ui-kit": "^4.15.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.4",
    "sass-functions": "^1.4.0",
    "serve-static": "^1.14.1",
    "sinon": "^9.2.3",
    "stylelint": "^13.8.0",
    "stylelint-config-nebenan": "^1.6.2",
    "terser": "^5.5.1",
    "tiny-lr": "^2.0.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "peerDependencies": {
