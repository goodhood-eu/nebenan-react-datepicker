{
  "name": "nebenan-react-datepicker",
  "description": "React datepicker component",
  "author": "Nebenan.de <peter.h@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-datepicker",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-datepicker/issues",
  "version": "1.1.0",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install && npm run decache",
    "decache": "find ./build -type f -name '.browserify-cache.json' -delete || echo \"\\033[0;32mAlready decached!\\033[0m\"",
    "check": "npm run test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "gulp": "gulp",
    "compile": "NODE_ENV=production npm run gulp compile",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run build",
    "lint": "stylelint \"./*.scss\" \"./**/*.scss\" && eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "NODE_ENV=production npm run gulp compile && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "start": "npm run gulp"
  },
  "devDependencies": {
    "browserslist-config-nebenan": "^1.0.0-beta.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-json-strings": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-strict-mode": "^7.8.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-node-env-inline": "0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-incremental": "^3.1.1",
    "chai": "^4.2.0",
    "clsx": "^1.1.0",
    "date-fns": "^2.11.0",
    "del": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "errorhandler": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-nebenan": "^1.2.1",
    "express": "^4.16.4",
    "fibers": "^4.0.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "jsdom": "^16.2.1",
    "mocha": "^7.1.1",
    "morgan": "^1.10.0",
    "nebenan-ui-kit": "^4.4.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sass": "^1.26.3",
    "sass-functions": "^1.3.0",
    "serve-static": "^1.13.2",
    "sinon": "^9.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-nebenan": "^1.6.0",
    "terser": "^4.6.7",
    "tiny-lr": "^1.1.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.9.2",
    "clsx": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "nebenan-ui-kit": "^4.4.1"
  },
  "dependencies": {}
}
